    "@types/react-native-video": "^5.0.14",
    "axios": "^1.3.4",
    "cheerio": "^1.0.0-rc.12",
    "dateformat": "^5.0.3",
    "deprecated-react-native-prop-types": "^4.0.0",
    "patch-package": "^6.5.1",
    "react": "18.2.0",
